{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es5",
      "dom"
    ],
    "types": [
      "node",
      "cypress"
    ],
    "strict": true,
    "allowJs": true,
    "noEmit": true,
    "isolatedModules": false,
    "forceConsistentCasingInFileNames": false
  },
  "include": [
    "**/*.ts",
    "support/cypress.d.ts"
  ]
}